<body>

    <!-- Sidebar -->
    <section id="sidebar">
        <div class="inner">
            <nav>
                <ul>
                    <li><a href="@Url.Action("Home", "Registration")">Welcome</a></li>
                    <li><a href="@Url.Action("ProgramDetails", "Registration")">Program Details</a></li>
                    <li><a href="@Url.Action("Venue", "Registration")">Venue</a></li>
                    <li><a href="@Url.Action("AboutUs", "Registration")">Who we are</a></li>
                    <li><a href="@Url.Action("Register", "Registration")">Register</a></li>
                    <li><a href="@Url.Action("Login", "Registration")">Login</a></li>
                </ul>
            </nav>
        </div>
    </section>
    <div>
        @RenderBody()
    </div>
